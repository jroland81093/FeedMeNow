CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/AFNetworking" "${PODS_ROOT}/Headers/BDBOAuth1Manager" "${PODS_ROOT}/Headers/FBGlowLabel" "${PODS_ROOT}/Headers/FlatUIKit" "${PODS_ROOT}/Headers/MACircleProgressIndicator" "${PODS_ROOT}/Headers/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/AFNetworking" -isystem "${PODS_ROOT}/Headers/BDBOAuth1Manager" -isystem "${PODS_ROOT}/Headers/FBGlowLabel" -isystem "${PODS_ROOT}/Headers/FlatUIKit" -isystem "${PODS_ROOT}/Headers/MACircleProgressIndicator" -isystem "${PODS_ROOT}/Headers/pop"
OTHER_LDFLAGS = -ObjC -lc++ -framework CoreGraphics -framework CoreText -framework MobileCoreServices -framework QuartzCore -framework Security -framework SystemConfiguration
PODS_ROOT = ${SRCROOT}/Pods